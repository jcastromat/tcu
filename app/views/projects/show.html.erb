<h1><%= @project.name %></h1>
<h2><%= @project.code %></h2>

<ul>
  <% @project.students.each do |student| %>
    <li>
      <%= link_to "#{student.firstName}", student_path(student) %>
    </li>
  <% end %>
</ul>
<br>

<p><%= button_to "Edit", edit_project_path, method: :get%> </p>
<p> <%= button_to "Delete", project_path(@project), method: :delete,
    data: {confirm: "Really delete the project?"} %>
</p>
<p><%= button_to "All Projects", projects_path, method: :get %> </p>
