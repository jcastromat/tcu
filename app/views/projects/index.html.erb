<h1>All Projects</h1>


<ul id="projects">
  <% @projects.each do |project| %>
    <li>
    <%= link_to project.name, project_path(project), class: 'project_name', :style=>'color:RED;'%>
    <br/>
    <%= render partial: 'projects/student', collection: project.students %>
    <br/>
    <%= render partial: 'students/form', locals: { project: project }%>
    </li>
  <% end %>
</ul>


<%= link_to "Create a New Project", new_project_path, class: "new_project" %>
